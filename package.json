{
  "name": "tailwind-nextjs-starter-blog",
  "version": "2.3.0",
  "private": true,
  "scripts": {
    "start": "npm run prebuild && next dev",
    "dev": "cross-env INIT_CWD=$PWD next dev",
    "build": "npm run prebuild && cross-env INIT_CWD=$PWD next build && cross-env NODE_OPTIONS='--experimental-json-modules' node ./scripts/postbuild.mjs",
    "serve": "next start",
    "analyze": "cross-env ANALYZE=true next build",
    "lint": "next lint --fix --dir pages --dir app --dir components --dir lib --dir layouts --dir scripts",
    "prepare": "husky",
    "prebuild": "node scripts/prebuild.mjs"
  },
  "engines": {
    "node": "20.x"
  },
  "dependencies": {
    "@headlessui/react": "2.2.0",
    "@next/bundle-analyzer": "15.0.2",
    "@plaiceholder/next": "^3.0.0",
    "@tailwindcss/forms": "0.5.9",
    "@tailwindcss/typography": "0.5.15",
    "@wojtekmaj/react-hooks": "1.18.0",
    "autoprefixer": "10.4.13",
    "body-scroll-lock": "4.0.0-beta.0",
    "clsx": "2.1.1",
    "contentlayer2": "0.5.3",
    "esbuild": "0.20.2",
    "github-slugger": "2.0.0",
    "glob": "^11.0.0",
    "gray-matter": "4.0.2",
    "hast-util-from-html-isomorphic": "2.0.0",
    "image-size": "1.0.0",
    "next": "15.0.2",
    "next-contentlayer2": "0.5.3",
    "next-themes": "0.3.0",
    "plaiceholder": "^3.0.0",
    "pliny": "0.4.0",
    "postcss": "8.4.24",
    "react": "rc",
    "react-dom": "rc",
    "reading-time": "1.5.0",
    "rehype-autolink-headings": "7.1.0",
    "rehype-citation": "2.0.0",
    "rehype-katex": "7.0.0",
    "rehype-katex-notranslate": "1.1.4",
    "rehype-preset-minify": "7.0.0",
    "rehype-prism-plus": "2.0.0",
    "rehype-slug": "6.0.0",
    "remark": "15.0.0",
    "remark-gfm": "4.0.0",
    "remark-github-blockquote-alert": "1.2.1",
    "remark-math": "6.0.0",
    "tailwindcss": "3.4.14",
    "unist-util-visit": "5.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.2.0",
    "@eslint/js": "9.16.0",
    "@svgr/webpack": "8.0.1",
    "@types/mdx": "2.0.12",
    "@types/react": "18.2.73",
    "@typescript-eslint/eslint-plugin": "8.12.0",
    "@typescript-eslint/parser": "8.12.0",
    "cross-env": "7.0.3",
    "eslint": "9.14.0",
    "eslint-config-next": "15.0.2",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "globals": "15.12.0",
    "husky": "9.1.7",
    "lint-staged": "13.0.0",
    "prettier": "3.0.0",
    "prettier-plugin-tailwindcss": "0.5.11",
    "raw-loader": "4.0.2",
    "spellchecker-cli": "6.2.0",
    "typescript": "5.1.3"
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "eslint --fix"
    ],
    "*.+(js|jsx|ts|tsx|json|css|md|mdx)": [
      "prettier --write"
    ]
  },
  "packageManager": "yarn@3.6.1"
}
