{
  "name": "gerrit",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build && next export",
    "start": "next start",
    "lint": "eslint components && eslint pages",
    "lint:fix": "eslint components --fix && eslint pages --fix && prettier --write pages && prettier --write components",
    "install:clean": "rm -rf node_modules && rm yarn.lock && yarn",
    "postinstall": "husky install"
  },
  "dependencies": {
    "i18next": "21.3.3",
    "i18next-browser-languagedetector": "6.1.2",
    "next": "12.0.0",
    "react": "npm:react@next",
    "react-dom": "npm:react-dom@next",
    "react-i18next": "11.12.0",
    "react-icons": "4.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "13.2.1",
    "@commitlint/config-conventional": "13.2.0",
    "babel-eslint": "10.1.0",
    "eslint": "8.1.0",
    "eslint-config-galex": "3.1.2",
    "eslint-config-next": "12.0.0",
    "husky": "7.0.4",
    "lint-staged": "11.2.6",
    "prettier": "2.4.1",
    "typescript": "4.4.4",
    "webpack": "5.60.0"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": "prettier --write",
    "*.jsx": "eslint --fix"
  }
}
