    "next": "12.0.9",
    "react": "npm:react@next",
    "react-dom": "npm:react-dom@next",
    "tailwindcss": "2.2.17"
  },
  "devDependencies": {
    "@types/react": "17.0.33",
    "@types/react-dom": "17.0.10",
    "autoprefixer": "10.3.7",
    "eslint": "8.1.0",
    "eslint-config-galex": "3.1.2",
